<body onload="bootstrap();">
  
  <script type="text/javascript" charset="utf-8">
    function bootstrap() {
      conn = new client.ConnectionManager({"force-new-connect":false});
		  conn.connect("<%= host %>", <%=port%>);
		  
		  setTimeout(function() {
		    conn.identify('Mark');
		    conn.join('MIT');
		  }, 100);
		  
      main_view = new views.MainView(conn);
      $("body").append(main_view.render().el); 
      
      window.kendoMobileApplication = new kendo.mobile.Application(document.body);  // Initialize Kendo ListView
    };
  </script>


